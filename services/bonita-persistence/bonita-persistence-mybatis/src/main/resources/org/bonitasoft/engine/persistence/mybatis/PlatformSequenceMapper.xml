<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.bonitasoft.engine.persistence.model.PlatformSequence">

	<cache />

	<select id="getPlatformSequence" parameterType="long" resultType="platformSequence">
		SELECT *
		FROM platformsequence
		WHERE id = #{id}
	</select>

	<update id="updatePlatformSequence" parameterType="platformSequence">
		UPDATE platformsequence
		SET nextId = #{nextId}
		WHERE id = #{id}
	</update>

</mapper>