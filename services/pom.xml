<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.bonitasoft.engine</groupId>
		<artifactId>bonita-engine-sp</artifactId>
		<version>6.0.0-beta-014</version>
	</parent>

	<artifactId>services</artifactId>
	<packaging>pom</packaging>

	<modules>
		<module>bonita-command</module>
		<module>bonita-data</module>
		<module>bonita-data-instance</module>
		<module>bonita-delete-batch</module>
		<module>bonita-dependency</module>
		<module>bonita-document</module>
		<module>bonita-events</module>
		<module>bonita-expression</module>
		<module>bonita-external-identity-mapping</module>
		<module>bonita-identity</module>
		<module>bonita-log</module>
		<module>bonita-persistence</module>
		<module>bonita-platform</module>
		<module>bonita-platform-command</module>
		<module>bonita-privilege</module>
		<module>bonita-profile</module>
		<module>bonita-scheduler</module>
		<module>bonita-work</module>
		<module>bonita-connector-executor-timedout</module>
	</modules>

	<profiles>
		<profile>
			<id>tests</id>
			<modules>
				<module>bonita-services-integration-tests</module>
			</modules>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-source-plugin</artifactId>
						<executions>
							<execution>
								<goals>
									<goal>jar</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>sonar</id>
			<modules>
				<module>bonita-services-integration-tests</module>
			</modules>
		</profile>
	</profiles>

</project>