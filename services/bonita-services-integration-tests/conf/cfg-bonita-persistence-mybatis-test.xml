<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="PersistenceTestFiles" class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="human" value="org.bonitasoft.engine.persistence.model.Human" />
				<entry key="parent" value="org.bonitasoft.engine.persistence.model.Parent" />
				<entry key="child" value="org.bonitasoft.engine.persistence.model.Child" />
				<entry key="car" value="org.bonitasoft.engine.persistence.model.Car" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/persistence/mybatis/HumanMapper.xml</value>
				<value>org/bonitasoft/engine/persistence/mybatis/CarMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.persistence.model.Human" value="human" />
				<entry key="org.bonitasoft.engine.persistence.model.Child" value="child" />
				<entry key="org.bonitasoft.engine.persistence.model.Parent" value="parent" />
				<entry key="org.bonitasoft.engine.persistence.model.Car" value="car" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<null />
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.getNumberOfParents" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getNumberOfHumans" />
					<constructor-arg name="parameterName" value="discriminant" />
					<constructor-arg name="parameterValue" value="parent" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.getParentsHavingAChildWithFirstName" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getParentsHavingAChildWithFirstName" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.getHumanById" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getHumanById" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.getNumberOfHumans" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getNumberOfHumans" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.getParentById" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getHumanById" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.deleteParent" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.deleteHuman" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.insertParent" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.insertParent" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Parent.getChildParent" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getChildParent" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Child.getChildById" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getHumanById" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Child.getAllChildren" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getAllHumans" />
					<constructor-arg name="parameterName" value="discriminant" />
					<constructor-arg name="parameterValue" value="child" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Child.insertChild" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.insertChild" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Child.deleteChild" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.deleteHuman" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Child.getParentChildren" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getParentChildren" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.persistence.model.Child.getNumberOfChildren" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.persistence.model.Human.getNumberOfHumans" />
					<constructor-arg name="parameterName" value="discriminant" />
					<constructor-arg name="parameterValue" value="child" />
				</bean>
			</set>
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<null />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<null />
		</constructor-arg>
	</bean>

	<bean class="org.bonitasoft.engine.persistence.DBConfiguration" depends-on="dbProperties">
		<constructor-arg name="createTablesFile" value="/org/bonitasoft/engine/persistence/test/human/${db.vendor}-createTables.sql" />
		<constructor-arg name="initTablesFile" value="/org/bonitasoft/engine/persistence/test/human/${db.vendor}-initTables.sql" />
		<constructor-arg name="dropTablesFile" value="/org/bonitasoft/engine/persistence/test/human/${db.vendor}-dropTables.sql" />
		<constructor-arg name="filter" value="journal" />
		<constructor-arg name="deleteTenantObjectsFile" value="/org/bonitasoft/engine/persistence/test/human/${db.vendor}-deleteTenantObjects.sql" />
		<constructor-arg name="deleteTenantObjectsPriority" value="0" />
		<property name="sqlTransformers">
			<map>
				<entry key="org.bonitasoft.engine.persistence.model.Human">
					<bean class="org.bonitasoft.engine.persistence.model.HumanSQLTransformer"></bean>
				</entry>
			</map>
		</property>
	</bean>

	<bean class="org.bonitasoft.engine.persistence.DBConfiguration" depends-on="dbProperties">
		<constructor-arg name="postCreateStructureFile" value="/org/bonitasoft/engine/persistence/test/human/${db.vendor}-postCreateStructure.sql" />
		<constructor-arg name="preDropStructureFile">
			<null />
		</constructor-arg>
		<constructor-arg name="filter" value="journal" />
	</bean>

</beans>