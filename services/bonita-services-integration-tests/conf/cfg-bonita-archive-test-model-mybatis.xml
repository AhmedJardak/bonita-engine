<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="employee" value="org.bonitasoft.engine.archive.model.Employee" />
				<entry key="laptop" value="org.bonitasoft.engine.archive.model.Laptop" />
				<entry key="address" value="org.bonitasoft.engine.archive.model.Address" />
				<entry key="project" value="org.bonitasoft.engine.archive.model.Project" />
				<entry key="employeeProjectMapping" value="org.bonitasoft.engine.archive.model.EmployeeProjectMapping" />
				<entry key="saemployee" value="org.bonitasoft.engine.archive.model.SAEmployee" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>com/bonitasoft/services/archive/model/mybatis/EmployeeMapper.xml</value>
				<value>com/bonitasoft/services/archive/model/mybatis/LaptopMapper.xml</value>
				<value>com/bonitasoft/services/archive/model/mybatis/AddressMapper.xml</value>
				<value>com/bonitasoft/services/archive/model/mybatis/ProjectMapper.xml</value>
				<value>com/bonitasoft/services/archive/model/mybatis/EmployeeProjectMappingMapper.xml</value>
				<value>com/bonitasoft/services/archive/model/mybatis/SAEmployeeMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.archive.model.Employee" value="employee" />
				<entry key="org.bonitasoft.engine.archive.model.Laptop" value="laptop" />
				<entry key="org.bonitasoft.engine.archive.model.Address" value="address" />
				<entry key="org.bonitasoft.engine.archive.model.Project" value="project" />
				<entry key="org.bonitasoft.engine.archive.model.EmployeeProjectMapping" value="employeeProjectMapping" />
				<entry key="org.bonitasoft.engine.archive.model.SAEmployee" value="saemployee" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map />
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.archive.model.Employee" value="org.bonitasoft.engine.archive.model.Employee" />
				<entry key="org.bonitasoft.engine.archive.model.Laptop" value="org.bonitasoft.engine.archive.model.Laptop" />
				<entry key="org.bonitasoft.engine.archive.model.Address" value="org.bonitasoft.engine.archive.model.Address" />
				<entry key="org.bonitasoft.engine.archive.model.Project" value="org.bonitasoft.engine.archive.model.Project" />
				<entry key="org.bonitasoft.engine.archive.model.EmployeeProjectMapping" value="org.bonitasoft.engine.archive.model.EmployeeProjectMapping" />
				<entry key="org.bonitasoft.engine.archive.model.SAEmployee" value="org.bonitasoft.engine.archive.model.SAEmployee" />
			</map>
		</constructor-arg>
	</bean>

</beans>