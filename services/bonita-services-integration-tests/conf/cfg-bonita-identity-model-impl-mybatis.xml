<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="user" value="org.bonitasoft.engine.identity.model.impl.SUserImpl" />
				<entry key="user_contactinfo" value="org.bonitasoft.engine.identity.model.impl.SContactInfoImpl" />
				<entry key="role" value="org.bonitasoft.engine.identity.model.impl.SRoleImpl" />
				<entry key="group_" value="org.bonitasoft.engine.identity.model.impl.SGroupImpl" />
				<entry key="user_membership" value="org.bonitasoft.engine.identity.model.impl.SUserMembershipImpl" />
				<entry key="p_metadata_def" value="org.bonitasoft.engine.identity.model.impl.SProfileMetadataDefinitionImpl" />
				<entry key="p_metadata_val" value="org.bonitasoft.engine.identity.model.impl.SProfileMetadataValueImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/identity/model/impl/mybatis/UserMapper.xml</value>
				<value>org/bonitasoft/engine/identity/model/impl/mybatis/RoleMapper.xml</value>
				<value>org/bonitasoft/engine/identity/model/impl/mybatis/GroupMapper.xml</value>
				<value>org/bonitasoft/engine/identity/model/impl/mybatis/UserMembershipMapper.xml</value>
				<value>org/bonitasoft/engine/identity/model/impl/mybatis/ProfileMetadataDefinitionMapper.xml</value>
				<value>org/bonitasoft/engine/identity/model/impl/mybatis/ProfileMetadataValueMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry key="org.bonitasoft.engine.identity.model.impl.SUserImpl" value="20" />
				<entry key="org.bonitasoft.engine.identity.model.impl.SRoleImpl" value="21" />
				<entry key="org.bonitasoft.engine.identity.model.impl.SGroupImpl" value="22" />
				<entry key="org.bonitasoft.engine.identity.model.impl.SUserMembershipImpl" value="23" />
				<entry key="org.bonitasoft.engine.identity.model.impl.SProfileMetadataDefinitionImpl" value="24" />
				<entry key="org.bonitasoft.engine.identity.model.impl.SProfileMetadataValueImpl" value="25" />
				<entry key="org.bonitasoft.engine.identity.model.impl.SContactInfoImpl" value="26" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.identity.model.SUser" value="u" />
				<entry key="org.bonitasoft.engine.identity.model.SRole" value="r" />
				<entry key="org.bonitasoft.engine.identity.model.SGroup" value="g" />
				<entry key="org.bonitasoft.engine.identity.model.SUserMembership" value="um" />
				<entry key="org.bonitasoft.engine.identity.model.SProfileMetadataDefinition" value="pmdd" />
				<entry key="org.bonitasoft.engine.identity.model.SProfileMetadataValue" value="pmdv" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo" value="ci" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.email" value="EMAIL" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.phoneNumber" value="PHONE" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.mobileNumber" value="MOBILE" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.faxNumber" value="FAX" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.building" value="BUILDING" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.room" value="ROOM" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.address" value="ADDRESS" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.zipCode" value="ZIPCODE" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.city" value="CITY" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.state" value="STATE" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.country" value="COUNTRY" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.website" value="WEBSITE" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.personal" value="PERSONAL" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo.userId" value="USERID" />
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set>
				<!-- insert -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.identity.model.impl.SContactInfoImpl.insertSContactInfoImpl" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.identity.model.impl.SUserImpl.insertSContactInfoImpl" />
				</bean>
				<!-- update -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.identity.model.impl.SContactInfoImpl.updateSContactInfoImpl" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.identity.model.impl.SUserImpl.updateSContactInfoImpl" />
				</bean>
				<!-- Queries -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement" value="org.bonitasoft.engine.identity.model.impl.SContactInfoImpl.getUserContactInfo" />
					<constructor-arg name="destinationStatement" value="org.bonitasoft.engine.identity.model.impl.SUserImpl.getUserContactInfo" />
				</bean>
			</set>
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.identity.model.SUser" value="org.bonitasoft.engine.identity.model.impl.SUserImpl" />
				<entry key="org.bonitasoft.engine.identity.model.SRole" value="org.bonitasoft.engine.identity.model.impl.SRoleImpl" />
				<entry key="org.bonitasoft.engine.identity.model.SGroup" value="org.bonitasoft.engine.identity.model.impl.SGroupImpl" />
				<entry key="org.bonitasoft.engine.identity.model.SUserMembership" value="org.bonitasoft.engine.identity.model.impl.SUserMembershipImpl" />
				<entry key="org.bonitasoft.engine.identity.model.SProfileMetadataDefinition" value="org.bonitasoft.engine.identity.model.impl.SProfileMetadataDefinitionImpl" />
				<entry key="org.bonitasoft.engine.identity.model.SProfileMetadataValue" value="org.bonitasoft.engine.identity.model.impl.SProfileMetadataValueImpl" />
				<entry key="org.bonitasoft.engine.identity.model.SContactInfo" value="org.bonitasoft.engine.identity.model.impl.SContactInfoImpl" />
			</map>
		</constructor-arg>
	</bean>

</beans>