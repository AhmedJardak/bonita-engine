<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.PlatformMyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="pdependency" value="org.bonitasoft.engine.dependency.model.impl.SPlatformDependency" />
				<entry key="pdependencymapping" value="org.bonitasoft.engine.dependency.model.impl.SPlatformDependencyMapping" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/dependency/model/impl/mybatis/PlatformDependencyMapper.xml</value>
				<value>org/bonitasoft/engine/dependency/model/impl/mybatis/PlatformDependencyMappingMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry key="org.bonitasoft.engine.dependency.model.impl.SPlatformDependency" value="10" />
				<entry key="org.bonitasoft.engine.dependency.model.impl.SPlatformDependencyMapping" value="11" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.dependency.model.SDependency" value="pdependency" />
				<entry key="org.bonitasoft.engine.dependency.model.SDependencyMapping" value="pdependencymapping" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map />
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.dependency.model.SDependency" value="org.bonitasoft.engine.dependency.model.impl.SPlatformDependency" />
				<entry key="org.bonitasoft.engine.dependency.model.SDependencyMapping" value="org.bonitasoft.engine.dependency.model.impl.SPlatformDependencyMapping" />
			</map>
		</constructor-arg>
	</bean>

</beans>