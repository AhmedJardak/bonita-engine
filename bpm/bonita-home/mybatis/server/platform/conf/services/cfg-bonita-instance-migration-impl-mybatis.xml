<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="migration_plan" value="org.bonitasoft.engine.core.migration.model.impl.SMigrationPlanDescriptorImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/core/migration/impl/mybatis/MigrationPlanMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.core.migration.model.SMigrationPlanDescriptor" value="mp" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.core.migration.model.SMigrationPlanDescriptor.sourceProcessName" value="source_name"/>
				<entry key="org.bonitasoft.engine.core.migration.model.SMigrationPlanDescriptor.sourceProcessVersion" value="source_version"/>
				<entry key="org.bonitasoft.engine.core.migration.model.SMigrationPlanDescriptor.targetProcessName" value="target_name"/>
				<entry key="org.bonitasoft.engine.core.migration.model.SMigrationPlanDescriptor.targetProcessVersion" value="target_version"/>
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set>
			</set>
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.core.migration.model.SMigrationPlanDescriptor" value="org.bonitasoft.engine.core.migration.model.impl.SMigrationPlanDescriptorImpl" />
			</map>
		</constructor-arg>
	</bean>

	<bean id="db-bonita-instance-migration-model" class="org.bonitasoft.engine.persistence.DBConfiguration">
		<constructor-arg name="createTablesFile" value="/org/bonitasoft/engine/core/migration/model/impl/db/${db.vendor}-createTables.sql" />
		<constructor-arg name="initTablesFile" value="/org/bonitasoft/engine/core/migration/model/impl/db/${db.vendor}-initTables.sql" />
		<constructor-arg name="cleanTablesFile" value="/org/bonitasoft/engine/core/migration/model/impl/db/${db.vendor}-cleanTables.sql" />
		<constructor-arg name="dropTablesFile" value="/org/bonitasoft/engine/core/migration/model/impl/db/${db.vendor}-dropTables.sql" />
		<constructor-arg name="filter" value="journal" />
		<constructor-arg name="deleteTenantObjectsFile" value="/org/bonitasoft/engine/core/migration/model/impl/db/${db.vendor}-deleteTenantObjects.sql" />
		<constructor-arg name="deleteTenantObjectsPriority" type="int" value="24" />
	</bean>
	
</beans>