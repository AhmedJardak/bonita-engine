<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="log" value="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogImpl" />
				<entry key="log_parameter" value="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogParameterImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/queriablelogger/model/impl/mybatis/QueriableLogMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry key="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogImpl" value="30" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogParameterImpl" value="31" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog" value="bl" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogParameterImpl" value="blp" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.applicationName" value="appName" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.productVersion" value="productVersion" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.clientApplicationName" value="clientAppName" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.callerClassName" value="callClassName" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.callerMethodName" value="callMethodName" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.numericIndex1" value="numIdx1" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.numericIndex2" value="numIdx2" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.numericIndex3" value="numIdx3" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.numericIndex4" value="numIdx4" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.numericIndex5" value="numIdx5" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.textualIndex1" value="txtIdx1" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.textualIndex2" value="txtIdx2" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.textualIndex3" value="txtIdx3" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.textualIndex4" value="txtIdx4" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog.textualIndex5" value="txtIdx5" />
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLog" value="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogImpl" />
				<entry key="org.bonitasoft.engine.queriablelogger.model.SQueriableLogParameter" value="org.bonitasoft.engine.queriablelogger.model.impl.SQueriableLogParameterImpl" />
			</map>
		</constructor-arg>
	</bean>

</beans>