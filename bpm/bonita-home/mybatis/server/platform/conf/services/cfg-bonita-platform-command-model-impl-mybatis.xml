<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.bonitasoft.engine.persistence.PlatformMyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="platformcommand" value="org.bonitasoft.engine.platform.command.model.impl.SPlatformCommandImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>com/bonitasoft/engine/platform/command/model/impl/mybatis/PlatformCommandMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.platform.command.model.SPlatformCommand" value="platformCommand" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.platform.command.model.SPlatformCommand" value="org.bonitasoft.engine.platform.command.model.impl.SPlatformCommandImpl" />
			</map>
		</constructor-arg>
	</bean>

</beans>