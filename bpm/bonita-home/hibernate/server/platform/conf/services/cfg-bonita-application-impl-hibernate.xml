<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="hbm-bos-application-model" class="org.bonitasoft.engine.persistence.HibernateResourcesProvider">
        <property name="resources">
            <set>
                <value>com/bonitasoft/engine/business/application/impl/hibernate/application.hbm.xml</value>
                <value>com/bonitasoft/engine/business/application/impl/hibernate/application.queries.hbm.xml</value>
            </set>
        </property>
        <property name="classAliasMappings">
            <map>
                <entry key="com.bonitasoft.engine.business.application.SApplication" value="application" />
                <entry key="com.bonitasoft.engine.business.application.SApplicationPage" value="applicationPage" />
            </map>
        </property>
    </bean>

</beans>