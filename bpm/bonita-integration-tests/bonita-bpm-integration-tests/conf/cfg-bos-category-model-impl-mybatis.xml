<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="category" value="org.bonitasoft.engine.core.category.model.impl.SCategoryImpl" />
				<entry key="processcategorymapping" value="org.bonitasoft.engine.core.category.model.impl.SProcessCategoryMappingImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/core/category/model/impl/mybatis/CategoryMapper.xml</value>
				<value>org/bonitasoft/engine/core/category/model/impl/mybatis/CategoryMappingMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.core.category.model.SCategory" value="category" />
				<entry key="org.bonitasoft.engine.core.category.model.SProcessCategoryMapping" value="processcategorymapping" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map />
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.core.category.model.SCategory" value="org.bonitasoft.engine.core.category.model.impl.SCategoryImpl" />
				<entry key="org.bonitasoft.engine.core.category.model.SProcessCategoryMapping" value="org.bonitasoft.engine.core.category.model.impl.SProcessCategoryMappingImpl" />
			</map>
		</constructor-arg>
	</bean>

	<bean class="org.bonitasoft.engine.persistence.DBConfiguration">
		<constructor-arg name="createTablesFile" value="/org/bonitasoft/engine/core/category/db/${db.vendor}-createTables.sql" />
		<constructor-arg name="initTablesFile" value="/org/bonitasoft/engine/core/category/db/${db.vendor}-initTables.sql" />
		<constructor-arg name="dropTablesFile" value="/org/bonitasoft/engine/core/category/db/${db.vendor}-dropTables.sql" />
		<constructor-arg name="filter" value="journal" />
		<constructor-arg name="deleteTenantObjectsFile" value="/org/bonitasoft/engine/core/category/db/${db.vendor}-deleteTenantObjects.sql" />
		<constructor-arg name="deleteTenantObjectsPriority" type="int" value="19"/>
	</bean>

</beans>