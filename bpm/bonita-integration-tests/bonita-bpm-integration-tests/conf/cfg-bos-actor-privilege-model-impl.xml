<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="actorPrivilegeBuilders" class="org.bonitasoft.engine.actor.privilege.model.builder.impl.ActorPrivilegeBuildersImpl" />

	<bean id="db-bos-actor-privilege-model" class="org.bonitasoft.engine.persistence.DBConfiguration">
		<constructor-arg name="createTablesFile" value="/org/bonitasoft/engine/actor/privilege/model/impl/db/${db.vendor}-createTables.sql" />
		<constructor-arg name="initTablesFile" value="/org/bonitasoft/engine/actor/privilege/model/impl/db/${db.vendor}-initTables.sql" />
		<constructor-arg name="dropTablesFile" value="/org/bonitasoft/engine/actor/privilege/model/impl/db/${db.vendor}-dropTables.sql" />
		<constructor-arg name="filter" value="journal" />
		<constructor-arg name="deleteTenantObjectsFile" value="/org/bonitasoft/engine/actor/privilege/model/impl/db/${db.vendor}-deleteTenantObjects.sql" />
		<constructor-arg name="deleteTenantObjectsPriority" type="int" value="8"/>
	</bean>
</beans>
