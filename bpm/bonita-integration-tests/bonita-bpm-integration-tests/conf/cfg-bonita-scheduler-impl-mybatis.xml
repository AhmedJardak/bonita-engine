<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="com.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="job_descriptor" value="org.bonitasoft.engine.scheduler.model.impl.SJobDescriptorImpl" />
				<entry key="job_parameter" value="org.bonitasoft.engine.scheduler.model.impl.SJobParameterImpl" />
				<entry key="job_log" value="org.bonitasoft.engine.scheduler.model.impl.SJobLogImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>com/bonitasoft/engine/scheduler/impl/mybatis/SJobDescriptorMapper.xml</value>
				<value>com/bonitasoft/engine/scheduler/impl/mybatis/SJobParametersMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.scheduler.model.SJobDescriptor" value="jd" />
				<entry key="org.bonitasoft.engine.scheduler.model.SJobParameter" value="jp" />
				<entry key="org.bonitasoft.engine.scheduler.model.SJobLog" value="jl" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map />
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map />
		</constructor-arg>
	</bean>

</beans>