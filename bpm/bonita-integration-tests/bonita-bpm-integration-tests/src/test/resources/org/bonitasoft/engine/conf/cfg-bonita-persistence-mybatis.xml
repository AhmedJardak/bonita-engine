                <entry key="org.bonitasoft.engine.dependency.model.impl.SDependencyMappingImpl" value="100"/>
                <entry key="org.bonitasoft.engine.identity.model.impl.SUserImpl" value="100"/>
                <entry key="org.bonitasoft.engine.identity.model.impl.SGroupImpl" value="100"/>
                <entry key="org.bonitasoft.engine.identity.model.impl.SRoleImpl" value="100"/>
                <entry key="org.bonitasoft.engine.identity.model.impl.SUserMembershipImpl" value="100"/>
                <entry key="org.bonitasoft.engine.identity.model.impl.SProfileMetadataDefinitionImpl" value="100"/>
                <entry key="org.bonitasoft.engine.identity.model.impl.SProfileMetadataValueImpl" value="100"/>
                <entry key="org.bonitasoft.engine.businesslogger.model.impl.SBusinessLogImpl" value="100"/>
                <entry key="org.bonitasoft.engine.businesslogger.model.impl.SBusinessLogParameterImpl" value="100"/>
                <entry key="org.bonitasoft.engine.privilege.model.impl.SPrivilegeImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.model.impl.SDataSourceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.model.impl.SDataSourceParameterImpl" value="100"/>
                <entry key="org.bonitasoft.engine.scheduler.impl.model.SJobDescriptorImpl" value="100"/>
                <entry key="org.bonitasoft.engine.scheduler.impl.model.SJobParameterImpl" value="100"/>
                <entry key="org.bonitasoft.engine.command.model.SCommandImpl" value="100"/>
                <entry key="org.bonitasoft.engine.platform.command.model.SPlatformCommandImpl" value="100"/>
                <entry key="org.bonitasoft.engine.actor.privilege.model.impl.SActorPrivilegeImpl" value="100"/>
                <entry key="org.bonitasoft.engine.actor.mapping.model.impl.SActorImpl" value="100"/>
                <entry key="org.bonitasoft.engine.actor.mapping.model.impl.SActorMemberImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.impl.SProcessInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.definition.model.impl.SProcessDefinitionImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.impl.STransitionInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.impl.SConnectorInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.event.trigger.impl.SEventTriggerInstanceImpl" value="100"/>
