                <entry key="org.bonitasoft.engine.core.process.instance.model.event.handling.impl.SMessageInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.impl.SPendingActivityMappingImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.breakpoint.impl.SBreakpointImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.instance.model.impl.SHiddenTaskInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SADataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SIntegerDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SBooleanDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SDoubleDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SShortTextDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SLongDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SBlobDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SDATEDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SLongTextDataInstance" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SXMLDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SDataInstanceVisibilityMappingImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.category.model.impl.SCategoryImpl" value="100"/>
                <entry key="org.bonitasoft.engine.document.model.impl.SDocumentImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.document.mapping.model.impl.SDocumentMappingImpl" value="100"/>
