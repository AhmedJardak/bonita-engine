                <entry key="org.bonitasoft.engine.supervisor.mapping.model.impl.SSupervisorImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.comment.model.impl.SCommentImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.comment.model.archive.impl.SACommentImpl" value="100"/>
                <entry key="org.bonitasoft.engine.external.identity.mapping.model.impl.SExternalIdentityMappingImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.migration.model.impl.SMigrationPlanImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.migration.model.impl.SMigrationPlanDescriptorImpl" value="100"/>
                <entry key="org.bonitasoft.engine.core.process.document.mapping.model.archive.impl.SADocumentMappingImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.impl.SADataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.archive.impl.SABooleanDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.archive.impl.SAIntegerDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.archive.impl.SADoubleDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.archive.impl.SAShortTextDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.archive.impl.SALongDataInstanceImpl" value="100"/>
                <entry key="org.bonitasoft.engine.data.instance.model.archive.impl.SADateDataInstanceImpl" value="100"/>
