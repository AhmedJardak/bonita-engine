<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="commandService" class="org.bonitasoft.engine.command.api.impl.CommandServiceImpl">
		<constructor-arg name="eventService" ref="eventService" />
		<constructor-arg name="persistence" ref="persistenceService" />
		<constructor-arg name="recorder" ref="recorderSync" />
		<constructor-arg name="commandBuilderAccessor" ref="commandBuilderAccessor" />
		<constructor-arg name="defaultCommandProvider">
			<bean class="org.bonitasoft.engine.command.api.impl.DefaultCommandProvider" autowire="byType" />
		</constructor-arg>
	</bean>

	<bean name="commandBuilderAccessor" class="org.bonitasoft.engine.command.model.SCommandBuilderAccessorImpl" />

	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>getActorIdsForUserIdIncludingTeam</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Get Map of all (processDefinitionId, Set of ActorIds) for user passed as parameter and all users of his team (he is a manager of). Use parameter key USER_ID_KEY</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.actor.GetActorIdsForUserIdIncludingTeam</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>evaluateExpressionsOnProcessInstance</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Evaluate expression using data from process instance.</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.expression.EvaluateExpressionsOnProcessInstance</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>evaluateExpressionsOnProcessDefinition</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Evaluate expression using data with default expression and parameter from process Definition.</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.expression.EvaluateExpressionsOnProcessDefinition</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>evaluateExpressionsOnActivityInstance</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Evaluate expression using data from activity instance.</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.expression.EvaluateExpressionsOnActivityInstance</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>evaluateExpressionsOnCompletedActivityInstance</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Evaluate expression using data from activity instance when activity completed.</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.expression.EvaluateExpressionsOnCompletedActivityInstance</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>evaluateExpressionOnCompletedProcessInstance</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Evaluate expression using data from process instance when process completed.</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.expression.EvaluateExpressionOnCompletedProcessInstance</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>evaluateExpressionAtProcessInstanciation</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Evaluate expression using data from process instance when process instantiated.</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.expression.EvaluateExpressionAtProcessInstanciation</value>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name">
			<value>isUserProcessSupervisor</value>
		</constructor-arg>
		<constructor-arg name="description">
			<value>Get if the user a supervisor of processDefinition. Use parameter keys USER_ID_KEY, PROCESS_DEFINITION_ID_KEY</value>
		</constructor-arg>
		<constructor-arg name="implementation">
			<value>org.bonitasoft.engine.external.process.IsUserProcessSupervisor</value>
		</constructor-arg>
	</bean>
	
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name"><value>addProfile</value></constructor-arg>
		<constructor-arg name="description"><value>Add profile</value></constructor-arg>
		<constructor-arg name="implementation" ><value>org.bonitasoft.engine.external.web.profile.command.AddProfile</value></constructor-arg>
