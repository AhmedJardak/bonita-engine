		<constructor-arg name="name"><value>searchProfile</value></constructor-arg>
		<constructor-arg name="description"><value>Search profile</value></constructor-arg>
		<constructor-arg name="implementation" ><value>org.bonitasoft.engine.external.web.profile.command.SearchProfile</value></constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.command.api.impl.CommandDescriptor">
		<constructor-arg name="name"><value>addProfileEntry</value></constructor-arg>
		<constructor-arg name="description"><value>Add profile Entry</value></constructor-arg>
		<constructor-arg name="implementation" ><value>org.bonitasoft.engine.external.web.profileentry.command.AddProfileEntry</value></constructor-arg>
