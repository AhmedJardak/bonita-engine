<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="categoryService" class="org.bonitasoft.engine.core.category.impl.CategoryServiceImpl">
		<constructor-arg name="persistence" ref="persistenceService" />
		<constructor-arg name="recorder" ref="recorderSync" />
		<constructor-arg name="eventService" ref="eventService" />
		<constructor-arg name="sessionService" ref="sessionService"/>
		<constructor-arg name="sessionAccessor" ref="sessionAccessor" />
		<constructor-arg name="parserFactory" ref="parserFactory" />
		<constructor-arg name="xmlWriter" ref="xmlWriter" />
		<constructor-arg name="categoryBuilderAccessor" ref="categoryBuilderAccessor" />
	</bean>

	<bean name="categoryBuilderAccessor" class="org.bonitasoft.engine.core.category.model.builder.impl.SCategoryBuilderAccessorImpl" />

</beans>