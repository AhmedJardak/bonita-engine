<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
     http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="process_instance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SProcessInstanceImpl" />
				<entry key="activity_instance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl" />
				<entry key="automatic_task_instance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SAutomaticTaskInstanceImpl" />
				<entry key="user_task_instance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>																	  
				<value>org/bonitasoft/engine/core/process/instance/model/impl/mybatis/ProcessInstanceMapper.xml</value>
				<value>org/bonitasoft/engine/core/process/instance/model/impl/mybatis/ActivityInstanceMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.impl.SProcessInstanceImpl"
					value="10010" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl"
					value="10011" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.impl.SAutomaticTaskInstanceImpl"
					value="10011" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl"
					value="10011" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SProcessInstance"
					value="p" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SActivityInstance"
					value="activityInst" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SAutomaticTaskInstance"
					value="autoTaskIinst" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SUserTaskInstance"
					value="useTaskInst" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set>
			 	<!-- insert -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SAutomaticTaskInstanceImpl.insertSAutomaticTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.insertSActivityInstanceImpl" />
					<constructor-arg name="parameterName" value="kind" />
					<constructor-arg name="parameterValue" value="auto" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl.insertSUserTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.insertSActivityInstanceImpl" />
					<constructor-arg name="parameterName" value="kind" />
					<constructor-arg name="parameterValue" value="user" />
				</bean>
				
				<!-- update -->
				
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl.updateSUserTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.updateSActivityInstanceImpl" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SAutomaticTaskInstanceImpl.updateSAutomaticTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.updateSActivityInstanceImpl" />
				</bean>
				
				<!-- delete -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl.deleteSUserTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.deleteSActivityInstanceImpl" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SAutomaticTaskInstanceImpl.deleteSAutomaticTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.deleteSActivityInstanceImpl" />
				</bean>
				
				<!-- get -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl.getPendingUserTasks" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl.getPendingUserTasks" />
				</bean>

			</set>
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SProcessInstance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SProcessInstanceImpl" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SActivityInstance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SActivityInstanceImpl" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SAutomaticTaskInstance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SAutomaticTaskInstanceImpl" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.SUserTaskInstance"
					value="org.bonitasoft.engine.core.process.instance.model.impl.SUserTaskInstanceImpl" />
			</map>
		</constructor-arg>
	</bean>
	
	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="arch_process_instance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAProcessInstanceImpl" />
				<entry key="arch_activity_instance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl" />
				<entry key="arch_automatic_task_instance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAAutomaticTaskInstanceImpl" />
				<entry key="arch_user_task_instance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAUserTaskInstanceImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>																	  
				<value>org/bonitasoft/engine/core/process/instance/model/impl/mybatis/ArchivedProcessInstanceMapper.xml</value>
				<value>org/bonitasoft/engine/core/process/instance/model/impl/mybatis/ArchivedActivityInstanceMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAProcessInstanceImpl"
					value="20010" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl"
					value="20011" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAAutomaticTaskInstanceImpl"
					value="20011" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAUserTaskInstanceImpl"
					value="20011" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAProcessInstance"
					value="p" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAActivityInstance"
					value="arch_activity_instance" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAAutomaticTaskInstance"
					value="arch_automatic_task_instance" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAUserTaskInstance"
					value="arch_user_task_instance" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set>
				<!-- insert -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAAutomaticTaskInstanceImpl.insertSAAutomaticTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl.insertAActivityInstanceImpl" />
					<constructor-arg name="parameterName" value="kind" />
					<constructor-arg name="parameterValue" value="auto" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAUserTaskInstanceImpl.insertSAUserTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl.insertAActivityInstanceImpl" />
					<constructor-arg name="parameterName" value="kind" />
					<constructor-arg name="parameterValue" value="user" />
				</bean>
				
				<!-- delete -->
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAAutomaticTaskInstanceImpl.deleteSAutomaticTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl.deleteAActivityInstanceImpl" />
				</bean>
				<bean class="org.bonitasoft.engine.persistence.StatementMapping">
					<constructor-arg name="sourceStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAUserTaskInstanceImpl.deleteSAUserTaskInstanceImpl" />
					<constructor-arg name="destinationStatement"
						value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl.deleteAActivityInstanceImpl" />
				</bean>

			</set>
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAProcessInstance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAProcessInstanceImpl" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAActivityInstance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAActivityInstanceImpl" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAAutomaticTaskInstance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAAutomaticTaskInstanceImpl" />
				<entry
					key="org.bonitasoft.engine.core.process.instance.model.archive.SAUserTaskInstance"
					value="org.bonitasoft.engine.core.process.instance.model.archive.impl.SAUserTaskInstanceImpl" />
			</map>
		</constructor-arg>
	</bean>
	

</beans>