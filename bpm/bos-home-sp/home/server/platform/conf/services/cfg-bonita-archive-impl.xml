		<constructor-arg name="definitiveArchiveDescriptors">
			<list>
				<bean id="year2011" class="org.bonitasoft.engine.archive.impl.ArchiveDescriptorImpl">
					<constructor-arg name="name" value="persistenceService" />
					<constructor-arg name="startTime" value="1293880039160" /> <!-- 01/01/2011 -->
					<constructor-arg name="endTime" value="1325329667316" /> <!-- 12/31/2011 -->
				</bean>
			</list>
		</constructor-arg>
		<constructor-arg name="slidingArchivePersistenceService" ref="persistenceService" />
