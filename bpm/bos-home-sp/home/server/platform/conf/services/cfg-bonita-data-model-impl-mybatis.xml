<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="dataSource" value="org.bonitasoft.engine.data.model.impl.SDataSourceImpl" />
				<entry key="dataSourceParameter" value="org.bonitasoft.engine.data.model.impl.SDataSourceParameterImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/data/model/impl/mybatis/SDataSourceMapper.xml</value>
				<value>org/bonitasoft/engine/data/model/impl/mybatis/SDataSourceParameterMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry key="org.bonitasoft.engine.data.model.impl.SDataSourceImpl" value="50" />
				<entry key="org.bonitasoft.engine.data.model.impl.SDataSourceParameterImpl" value="51" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.data.model.SDataSource" value="datasource" />
				<entry key="org.bonitasoft.engine.data.model.SDataSourceParameter" value="datasourceparameter" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map />
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.data.model.SDataSource" value="org.bonitasoft.engine.data.model.impl.SDataSourceImpl" />
				<entry key="org.bonitasoft.engine.data.model.SDataSourceParameter" value="org.bonitasoft.engine.data.model.impl.SDataSourceParameterImpl" />
			</map>
		</constructor-arg>
	</bean>

</beans>