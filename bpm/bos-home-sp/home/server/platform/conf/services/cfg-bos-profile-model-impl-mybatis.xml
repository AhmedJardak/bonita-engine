<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.bonitasoft.engine.persistence.MyBatisConfiguration">
		<constructor-arg name="typeAliases">
			<map>
				<entry key="profile" value="org.bonitasoft.engine.profile.model.impl.SProfileImpl" />
				<entry key="profileentry" value="org.bonitasoft.engine.profile.model.impl.SProfileEntryImpl" />
				<entry key="profilemember" value="org.bonitasoft.engine.profile.model.impl.SProfileMemberImpl" />
			</map>
		</constructor-arg>
		<constructor-arg name="mappers">
			<list>
				<value>org/bonitasoft/engine/profile/model/impl/mybatis/SProfileMapper.xml</value>
				<value>org/bonitasoft/engine/profile/model/impl/mybatis/SProfileEntryMapper.xml</value>
				<value>org/bonitasoft/engine/profile/model/impl/mybatis/SProfileMemberMapper.xml</value>
			</list>
		</constructor-arg>
		<constructor-arg name="sequencesMappings">
			<map>
				<entry key="org.bonitasoft.engine.profile.model.impl.SProfileImpl" value="9990" />
				<entry key="org.bonitasoft.engine.profile.model.impl.SProfileEntryImpl" value="9991" />
				<entry key="org.bonitasoft.engine.profile.model.impl.SProfileMemberImpl" value="9992" />
			</map>
		</constructor-arg>
		<constructor-arg name="classAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.profile.model.SProfile" value="p" />
				<entry key="org.bonitasoft.engine.profile.model.SProfileEntry" value="pe" />
				<entry key="org.bonitasoft.engine.profile.model.SProfileMember" value="pm" />
			</map>
		</constructor-arg>
		<constructor-arg name="classFieldAliasMappings">
			<map>
				<entry key="org.bonitasoft.engine.profile.model.SProfileEntry.index" value="index_" />
			</map>
		</constructor-arg>
		<constructor-arg name="statementMapping">
			<set />
		</constructor-arg>
		<constructor-arg name="dbStatementsMapping">
			<map />
		</constructor-arg>
		<constructor-arg name="entityMappings">
			<map>
				<entry key="org.bonitasoft.engine.profile.model.SProfile" value="org.bonitasoft.engine.profile.model.impl.SProfileImpl" />
				<entry key="org.bonitasoft.engine.profile.model.SProfileEntry" value="org.bonitasoft.engine.profile.model.impl.SProfileEntryImpl" />
				<entry key="org.bonitasoft.engine.profile.model.SProfileMember" value="org.bonitasoft.engine.profile.model.impl.SProfileMemberImpl" />
			</map>
		</constructor-arg>
	</bean>

</beans>