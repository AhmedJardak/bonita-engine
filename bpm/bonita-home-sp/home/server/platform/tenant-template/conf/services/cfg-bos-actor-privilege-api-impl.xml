<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="actorPrivilegeService" class="org.bonitasoft.engine.actor.privilege.api.impl.ActorPrivilegeServiceImpl">
		<constructor-arg name="actorPrivilegeBuilders" ref="actorPrivilegeBuilders" />
		<constructor-arg name="privilegeService" ref="privilegeService" />
		<constructor-arg name="actorMappingService" ref="actorMappingService" />
		<constructor-arg name="recorder" ref="recorderSync" />
		<constructor-arg name="eventBuilders" ref="eventBuilders" />
		<constructor-arg name="persistenceService" ref="persistenceService" />
	</bean>

</beans>
