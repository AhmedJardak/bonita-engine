<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="migrationPlanService" class="org.bonitasoft.engine.core.migration.impl.MigrationPlanServiceImpl">
		<constructor-arg name="recorder" ref="recorderAsync" />
		<constructor-arg name="persistenceService" ref="persistenceService" />
		<constructor-arg name="eventService" ref="eventService" />
		<constructor-arg name="parserFactory" ref="parserFactory" />
		<constructor-arg name="sExpressionBuilders" ref="sExpressionBuilders" />
		<constructor-arg name="sOperationBuilders" ref="sOperationBuilders" />
		<constructor-arg name="cacheService" ref="cacheService" />
		<constructor-arg name="sessionAccessor" ref="sessionAccessor" />
		<constructor-arg name="processInstanceService" ref="processInstanceService" />
		<constructor-arg name="breakpointService" ref="breakpointService" />
		<constructor-arg name="breakpointBuilder" ref="bpmInstanceBuilders" />
		<constructor-arg name="interruptingStateId" value="45" />
        <constructor-arg name="businessLoggerService" ref="syncBusinessLoggerService" />
	</bean>
	
</beans>