<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false">

   <query name="getReportById">
		SELECT report
		FROM org.bonitasoft.engine.core.reporting.SReportImpl AS report
		WHERE report.id = :id
	</query>

	<query name="getNumberOfSReport">
		SELECT COUNT(report)
		FROM org.bonitasoft.engine.core.reporting.SReportImpl AS report
	</query>

	<query name="searchSReport">
		SELECT report
		FROM org.bonitasoft.engine.core.reporting.SReportImpl AS report
	</query>

</hibernate-mapping>
